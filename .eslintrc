{
  "extends": ["unobtrusive", "unobtrusive/import", "unobtrusive/flowtype"],
  "plugins": ["jest"],
  "rules": {
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
    "flowtype/no-dupe-keys": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  }
}
